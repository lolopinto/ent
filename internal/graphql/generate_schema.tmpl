{{ reserveImport .QueryPath "QueryType" }}
{{ reserveImport .MutationPath "MutationType" }}
{{ reserveImport "graphql" "GraphQLSchema" }}
{{ reserveImport "graphql/utilities" "printSchema" }}
{{ reserveDefaultImport "fs" "fs" }}

const schema = new {{useImport "GraphQLSchema"}}({
  query: {{useImport "QueryType"}},
  {{ if .HasMutations}} 
    mutation: {{useImport "MutationType"}},
  {{end -}}
});

let str = {{useImport "printSchema"}}(schema);

{{useImport "fs"}}.writeFileSync("{{.SchemaPath}}", str);