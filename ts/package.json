{
  "name": "@snowtop/ent",
  "version": "0.2.3-alpha1",
  "description": "snowtop ent framework",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./*": "./*"
  },
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "@types/node": "^25.0.3",
    "cosmiconfig": "^9.0.0",
    "dataloader": "^2.2.3",
    "glob": "^13.0.0",
    "graph-data-structure": "^4.5.0",
    "js-yaml": "^4.1.1",
    "json5": "^2.2.3",
    "luxon": "^3.7.2",
    "memoizee": "^0.4.17",
    "minimist": "^1.2.8",
    "pg": "^8.16.3",
    "prettier": "^3.7.4",
    "ts-node": "^11.0.0-beta.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@swc-node/register": "^1.6.8",
    "better-sqlite3": "^12.5.0",
    "graphql": "^16.8.1"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "@swc-node/register": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "devDependencies": {
    "@swc/core": "^1.15.7",
    "@swc/jest": "^0.2.39",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.7.1",
    "@types/memoizee": "^0.4.12",
    "@types/minimist": "^1.2.5",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/pluralize": "0.0.33",
    "@types/supertest": "^6.0.3",
    "better-sqlite3": "^12.5.0",
    "email-addresses": "^5.0.0",
    "express": "^5.2.1",
    "graphql": "^16.12.0",
    "graphql-helix": "^1.13.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^17.0.0",
    "jest": "^30.2.0",
    "jest-date-mock": "^1.0.10",
    "jest-each": "^30.2.0",
    "jest-expect-message": "^1.1.3",
    "jest-mock": "^30.2.0",
    "node-sql-parser": "^5.3.13",
    "object-path": "^0.11.8",
    "pg-copy-streams": "^7.0.0",
    "pluralize": "^8.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6"
  },
  "scripts": {
    "compile": "tsc",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "test:single": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "clean": "rm -rf dist",
    "prepare-code": "npm run clean && npm run compile && node src/setupPackage.js",
    "prepare-code-legacy": "npm run clean && npm run compile && node src/setupPackage.js --legacy",
    "actual-publish": "npm run prepare-code && npm publish ./dist --verbose --access public",
    "legacy-publish": "npm run prepare-code-legacy && npm publish ./dist --verbose --registry https://npm.pkg.github.com/"
  },
  "bin": {
    "ent-custom-graphql": "scripts/custom_graphql.js",
    "ent-custom-compiler": "scripts/custom_compiler.js"
  },
  "author": "lolopinto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lolopinto/ent.git"
  },
  "bugs": {
    "url": "https://github.com/lolopinto/ent/issues"
  },
  "homepage": "https://github.com/lolopinto/ent#readme"
}
