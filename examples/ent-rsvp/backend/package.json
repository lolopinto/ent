    "@types/express": "^4.17.9",
    "@types/graphql-upload": "^8.0.4",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "jest-expect-message": "^1.0.2",
    "ts-jest": "^26.4.4"
