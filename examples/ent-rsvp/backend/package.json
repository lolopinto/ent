{
  "name": "ent-rsvp",
  "version": "1.0.0",
  "description": "ent rsvp example",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "compile": "rm -rf dist && ./node_modules/.bin/ent-custom-compiler",
    "start": "docker-compose -f docker-compose.dev.yml up --build",
