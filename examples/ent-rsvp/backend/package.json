    "upgrade": "docker-compose -f docker-compose.dev.yml run --rm app tsent upgrade",
    "start-fast": "npm run compile && node dist/graphql/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolopinto/ent-starter.git"
