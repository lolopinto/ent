    "@snowtop/ent-graphql-tests": "^0.1.0-rc1",
    "@swc/core": "^1.3.57",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/graphql-upload": "^16.0.7",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^2.0.4",
    "@types/passport": "^1.0.7",
