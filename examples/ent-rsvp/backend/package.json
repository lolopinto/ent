  "bugs": {
    "url": "https://github.com/lolopinto/ent-starter/issues"
  },
  "homepage": "https://github.com/lolopinto/ent-starter#readme",
  "dependencies": {
    "@sentry/node": "^6.3.0",
    "@sentry/tracing": "^6.3.0",
    "@snowtop/ent": "^0.1.23",
    "@snowtop/ent-email": "^0.1.0",
    "@snowtop/ent-passport": "^0.1.0",
    "@snowtop/ent-password": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^8.0.3",
    "graphql": "^16.8.1",
